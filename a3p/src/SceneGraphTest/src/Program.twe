class Program {
  Program() {
  }


  static void main() {
    Program story <- new Program();
    story.execute();
  }
  
  void execute() {
  	Box box <- new Box();
    box.setSize(size: new Size(width: 2, height: 1.5, depth: 0.5));
    box.setPaint(paint: new Color(red: 1, green: 0, blue: 0), duration: 4.2);
  }
}