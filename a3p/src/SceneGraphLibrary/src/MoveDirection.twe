enum MoveDirection {
	LEFT( axis: Direction.NEGATIVE_X_AXIS ),
	RIGHT( axis: Direction.POSITIVE_X_AXIS ),
	UP( axis: Direction.POSITIVE_Y_AXIS ),
	DOWN( axis: Direction.NEGATIVE_Y_AXIS ),
	FORWARD( axis: Direction.POSITIVE_Z_AXIS ),
	BACKWARD( axis: Direction.NEGATIVE_Z_AXIS );

	Direction axis;

	MoveDirection( Direction axis ) {
		this.axis <- axis;
	}

	Direction getAxis() {
		return this.axis;
	}
}