enum MoveDirection {
	LEFT( axis: Direction.LEFT ),
	RIGHT( axis: Direction.RIGHT ),
	UP( axis: Direction.UP ),
	DOWN( axis: Direction.DOWN ),
	FORWARD( axis: Direction.FORWARD ),
	BACKWARD( axis: Direction.BACKWARD );

	Direction axis;

	MoveDirection( Direction axis ) {
		this.axis <- axis;
	}

	Direction getAxis() {
		return this.axis;
	}
}