class Program {
  
  static void main() {
    $Clock.delay(duration: 1.5);
    forLoop(start: 0, end: 100);
  }

  static void forLoop(WholeNumber start, WholeNumber end) {
    WholeNumber i <- start;

    while (i <= end) {
      $Debug.log(string: "" .. i);
      i <- i + 1;
      Boolean rand <- $Clock.returnRandomBool(duration: 1);
      $Debug.log(string: "Random bool " .. rand);
    }

  }
}