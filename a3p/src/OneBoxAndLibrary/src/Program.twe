class Program extends SProgram {
  Program() {
  }
  Scene myScene <- new Scene();

  static void main(TextString[] args) {
  
	// Portion p <- 0;
	// DecimalNumber n <- 1.0;
	// n <- -1;
	  
    // Create a runtime window, then display and activate myScene in the window
    Program story <- new Program();
    story.initializeInFrame();
    story.setActiveScene(scene: story.getMyScene());
  }
  Scene getMyScene() {
    return this.myScene;
  }
}